{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es6", "dom", "es2016", "es2017"],
    "module": "commonjs",
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "experimentalDecorators": true,
    "noLib": false,
    "jsx": "react",
    "outDir": "dist",
    "skipLibCheck": true    
  },
  "exclude": [
    "node_modules",
    "static"
  ]
}

